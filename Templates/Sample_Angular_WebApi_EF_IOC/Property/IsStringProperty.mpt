<CONFIG>
	NAME IsStringProperty
	CATEGORY LIB
	NODE Property
	TOPLEVEL False
</CONFIG>
<CONTENT>
<%%:
var isString = "false"
switch (DataTypeCode)
{
	case 13: // Char
		if (Length > 1)
		{
			isString = "true"
		}
		break
	case 14: // CharUnicode
		if (Length > 1)
		{
			isString = "true"
		}
		break
	case 16: // String
	case 17: // StringUnicode
	case 19: // Text
	case 20: // TextUnicode
		isString = "true"
		break
}
<%%=isString%%>
%%></CONTENT>